	<body>
	    <div class="weui-flex dark-bg" style="padding:10px;vertical-align: middle;">
          <div class="weui-flex__item">
              <i class="icon fa fa-angle-left fa-2x white" onclick="javascript:history.go(-1)"></i>
          </div>
          <div class="weui-flex__item" align="center">
              <h4 class="title white">注册</h4>
          </div>
          <div class="weui-flex__item white" align="center" >
              <a href="/view/cust/cust_login.html" style="color:darkgrey; text-decoration: none;font-size:20px; float: right;" >登陆</a>
          </div>
        </div> 
        <div class="weui-cells weui-cells_form" style="margin-top:0;">
            <div class="weui-cell">
                <div class="weui-cell__hd">
                  <label class="weui-label">姓名：</label>
                </div>
                <div class="weui-cell__bd">
                  <input class="weui-input">
                </div>
            </div>
            <div class="weui-cell">
                <div class="weui-cell__hd">
                  <label class="weui-label">身份证号：</label>
                </div>
                <div class="weui-cell__bd">
                  <input class="weui-input">
                </div>
            </div>
            <div class="weui-cell">
                <div class="weui-cell__hd">
                  <label class="weui-label">手机号：</label>
                </div>
                <div class="weui-cell__bd">
                  <input class="weui-input" type="tel">
                </div>
            </div>
            <div class="weui-cell weui-cell_vcode" >
                <div class="weui-cell__hd"><label class="weui-label">短信验证码：</label></div>
                <div class="weui-cell__bd">
                  <input class="weui-input" type="number">
                </div>
                <div class="weui-cell__ft">
                  <button class="weui-vcode-btn">获取验证码</button>
                </div>
            </div>
            <div class="weui-cell">
                <div class="weui-cell__hd">
                  <label class="weui-label">密码：</label>
                </div>
                <div class="weui-cell__bd">
                  <input class="weui-input" type="password" >
                </div>
            </div>
            <div class="weui-cell">
                <div class="weui-cell__hd">
                  <label class="weui-label">确认密码：</label>
                </div>
                <div class="weui-cell__bd">
                  <input class="weui-input" type="password" >
                </div>
            </div>
            <div class="weui-btn-area">
              <a class="weui-btn btn-orange" href="javascript:register();" >注册</a>
            </div>
        </div>
	</body>
